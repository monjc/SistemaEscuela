<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoprueba.GestionHistoriaAcademicaController">
   <children>
      <Label layoutX="331.0" layoutY="35.0" text="Gestión de Historia Académica" />
      <TableView fx:id="tablaHistoriaAcademica" layoutX="215.0" layoutY="110.0" prefHeight="256.0" prefWidth="373.0">
         <columns>
            <TableColumn fx:id="columnaAlumno" prefWidth="75.0" text="Alumno" />
            <TableColumn fx:id="columnaColegioAnterior" prefWidth="75.0" text="ColegioAnterior" />
            <TableColumn fx:id="columnaAño" prefWidth="75.0" text="Año" />
            <TableColumn fx:id="columnaGrado" prefWidth="75.0" text="Grado" />
         </columns>
      </TableView>
      <Button layoutX="38.0" layoutY="40.0" mnemonicParsing="false" text="Regresar" />
      <ComboBox fx:id="comboAlumno" layoutX="38.0" layoutY="110.0" prefWidth="150.0" promptText="Alumno" />
      <Label layoutX="38.0" layoutY="81.0" text="Alumno" />
      <Label layoutX="38.0" layoutY="144.0" text="Colegio Anterior" />
      <TextField fx:id="txtColegioAnterior" layoutX="38.0" layoutY="171.0" />
      <Label layoutX="40.0" layoutY="202.0" text="Año" />
      <TextField fx:id="txtAño" layoutX="39.0" layoutY="226.0" />
      <Button fx:id="btnRegistrar" layoutX="75.0" layoutY="331.0" mnemonicParsing="false" onAction="#registrarHistoriaAcademica" text="Registrar" />
      <Button fx:id="btnEliminar" layoutX="376.0" layoutY="371.0" mnemonicParsing="false" onAction="#eliminarHistoriaAcademica" text="Eliminar" />
      <Label layoutX="38.0" layoutY="260.0" text="Grado" />
      <TextField fx:id="txtGrado" layoutX="39.0" layoutY="288.0" />
   </children>
</AnchorPane>
